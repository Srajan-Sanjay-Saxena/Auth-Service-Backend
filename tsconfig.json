
{
  "compilerOptions": {
    "esModuleInterop": true,
    "skipLibCheck": false,
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": true,
    "moduleDetection": "force",
    "isolatedModules": true,
    "verbatimModuleSyntax": true,
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "sourceMap": true,
    "outDir": "./JS",
  },
}
